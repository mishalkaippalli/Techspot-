    <main class="main-wrap">
        <header class="main-header navbar">
            <div class="col-nav">
                <button class="btn btn-icon btn-mobile me-auto" data-trigger="#offcanvas_aside"> <i class="material-icons md-apps"></i> </button>
            </div>
        </header>
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Users List </h2>
                    <p>You can 'Edit' and 'Block' the User</p>
                </div>
            </div>
            <div class="card mb-4">
                <!-- card-body start// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table id="listUserTable" class="table table-hover">
                            <thead class="table-dark">
                                <tr>
                                    <th>SI:</th>
                                    <th scope="col">User Name</th>
                                    <th scope="col">Email</th>
                                    <th scope="col">Mobile</th>
                                    <th scope="col">Edit</th>
                                    <th scope="col">Block/Unblock</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% if (users.length>0) { %>
                                    <% for( let i=0; i < users.length; i++ ) { %>
                                        <tr>
                                            <td><%- i+1 %></td>
                                            <td><b><%- users[i].firstName + ' ' +users[i].lastName %></b></td>
                                            <td><b><%- users[i].email %></b></td>
                                            <td><b><%- users[i].mobile %></b></td>
                                            <td class="">
                                                <a href="/admin/edit-users?id=<%- users[i]._id %>"
                                                    class="btn btn-md-edit rounded font-sm">Edit</a>
                                            </td>
                                            <td class="">
                                                <% if (users[i].is_blocked===true) { %>
                                                    <a href="/admin/unblock-users?id=<%- users[i]._id %>"
                                                        class="btn btn-md-unblock rounded font-sm unblock-btn">Unblock</a>
                                                    <% }else{ %>
                                                        <a href="/admin/block-users?id=<%- users[i]._id %>"
                                                            class="btn btn-md-block rounded font-sm block-btn">Block</a>
                                                        <% } %>

                                            </td>
                                        </tr>
                                        <% } %>
                                            <% } else { %>
                                                <tr>
                                                    <td style="text-align: center;" colspan="6">
                                                        <h1>No users found</h1>
                                                    </td>
                                                </tr>
                                                <% } %>
                            </tbody>
                        </table>
                    </div> <!-- table-responsive //end -->
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
        </section> <!-- content-main end// -->
        <footer class="main-footer font-xs">
            <div class="row pb-30 pt-15">
                <div class="col-sm-6">
