    <!-- Pi Chart of the Whole orders -->
    <script>
        document.addEventListener("DOMContentLoaded", function () {
            // References of order data
            const mCtx = document.getElementById('mChart');
            const delivered = parseInt('<%- ordCount.Delivered %>');
            const placed = parseInt('<%- ordCount.Placed %>');
            const returned = parseInt('<%- ordCount.Returned %>');
            const cancelled = parseInt('<%- ordCount.Cancelled %>');
            const pending = parseInt('<%- ordCount.Pending %>');

            // Checking if the canvas is existing or not 
            if (mCtx) {
                new Chart(mCtx, {
                    type: 'pie', // Change to 'pie' for pie chart
                    data: {
                        labels: ['Delivered', 'Placed', 'Returned', 'Cancelled', 'Pending'],
                        datasets: [{
                            label: 'Orders',
                            data: [delivered, placed, returned, cancelled, pending],
                            borderWidth: 1,
                            backgroundColor: [
                                'rgba(3, 252, 61, 0.8)',  // Red
                                'rgba(75, 192, 192, 0.8)', // Teal
                                'rgba(255, 206, 86, 0.8)',  // Yellow
                                'rgba(255, 99, 132, 1)',  // Blue
                                'rgba(153, 102, 255, 0.8)'  // Purple
                            ],
                        }]
                    },
                    options: {
                        plugins: {
                            title: {
                                display: true,
                                text: 'Sale Statistics',
                                font: {
                                    size: 30 // Change the font size here
                                }
                            }
                        }
                    }
                })
            }
        });

    </script>

    <!-- Category Wise Bar chart -->

    <script>
        document.addEventListener("DOMContentLoaded", function () {
            const cCtx = document.getElementById('cChart')
            const keys = JSON.parse(document.getElementById('keys').value);
            console.log(keys)
            const values = JSON.parse(document.getElementById('values').value);
            if (cCtx) {
                new Chart(cCtx, {
                    type: 'bar',
                    data: {
                        labels:keys,
                        datasets: [{
                            label: 'Number of Products',
                            data: values,
                            backgroundColor: 'rgba(75, 192, 192,1)',
                            borderWidth: 1,
                        }],
                    },
                    options: {
                        scales: {
                            y: {
                                beginAtZero: true,
                                title: {
                                    display: true,
                                    text: 'Number of Products',
                                },
                            },
                        },
                        plugins: {
                            legend: {
                                display: false,
                            },
                        },
                    },
                });
            }

        });

    </script>

